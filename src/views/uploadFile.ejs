<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
    <link rel="stylesheet" href="/css/index.css" />
  </head>
  <body>
    <div class="topnav">
      <a href="/">Home</a>
      <a href="/upload" class="active">upload file</a>
    </div>
    <div>
      <h1>Upload file</h1>
      <form
        action="/upload-profile-pic"
        enctype="multipart/form-data"
        method="post"
      >
        <div class="input-file">
          <input type="file" name="profile_pic" id="file" />
        </div>
        <div><button type="submit">upload</button></div>
      </form>
    </div>
    <hr />
    <div>
      <h1>Upload files</h1>
      <!-- action="/upload-profile-pic" -->
      <form
        method="POST"
        action="/upload-multiple-images"
        enctype="multipart/form-data"
      >
        <div class="input-file">
          <input type="file" name="multiple_images" multiple id="files" />
          <label for="files" class="input-label"> </label>
        </div>
        <div><button type="submit">upload</button></div>
      </form>
    </div>
  </body>
</html>
